{
  "name": "webpack2-vue2.x",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node build/dev-server.js",
    "pro": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --hide-modules",
    "rd": "cross-env NODE_ENV=rd webpack --config build/webpack.client.conf.js --hide-modules",
    "report": "npm run pro --report",
    "deploy:pro": "cd dist && fis3 release dist",
    "deploy:qa": "cd dist && fis3 release test",
    "deploy:rd": "cd dist && fis3 release rd",
    "doc": "rm -rf docs && jsdoc src/components -c config/jsdoc-config.json --readme README.md"
  },
